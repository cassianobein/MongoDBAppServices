{
    "collection": "Info",
    "database": "Darwin",
    "filters": [
        {
            "name": "filter",
            "query": {
                "owner_id": "%%user.id"
            },
            "apply_when": {
                "%%true": true
            }
        }
    ],
    "roles": [
        {
            "name": "readAll",
            "apply_when": {},
            "document_filters": {
                "write": false,
                "read": true
            },
            "fields": {
                "first_name": {
                    "write": false,
                    "read": true
                },
                "last_name": {
                    "write": false,
                    "read": true
                }
            },
            "insert": false,
            "delete": false,
            "search": true
        }
    ]
}
